<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="400.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jellybeanci.Controller">
    <top>
        <HBox alignment="CENTER_LEFT" maxWidth="10000" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" spacing="20.0" BorderPane.alignment="CENTER">
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <BorderPane.margin>
                <Insets left="10.0" right="10.0" />
            </BorderPane.margin>
            <padding>
                <Insets left="3.0" right="3.0" />
            </padding>
            <Label text="Dataset URL" />
            <TextField fx:id="textBox" maxWidth="10000" prefHeight="1.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnGetData" mnemonicParsing="false" onMouseClicked="#onGetDataClick" text="GetData" />
        </HBox>
    </top>
    <padding>
        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
    </padding>
    <center>
        <VBox fx:id="mainVBox" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <TableView fx:id="tableView" maxHeight="250.0" minHeight="200.0" prefHeight="200.0" VBox.vgrow="SOMETIMES">
                <columns>
                    <TableColumn fx:id="countryName" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Country" />
                    <TableColumn fx:id="totalCases" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Total Cases" />
                    <TableColumn fx:id="newCases" editable="false" maxWidth="500.0" prefWidth="-1.0" text="New Cases" />
                    <TableColumn fx:id="totalDeaths" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Total Deaths" />
                    <TableColumn fx:id="newDeaths" editable="false" maxWidth="500.0" prefWidth="-1.0" text="New Deaths" />
                    <TableColumn fx:id="population" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Population" />
                    <TableColumn fx:id="mortality" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Mortality" />
                    <TableColumn fx:id="attackRate" editable="false" maxWidth="500.0" prefWidth="-1.0" text="Attack Rate" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
            <TabPane prefHeight="236.0" prefWidth="586.0" tabClosingPolicy="UNAVAILABLE">
                <tabs>
                    <Tab text="Untitled Tab 1">
                        <content>
                            <AnchorPane minHeight="300.0" minWidth="250.0" prefHeight="300.0">
                                <children>
                                    <ButtonBar layoutX="90.0" layoutY="55.0" prefHeight="40.0" prefWidth="200.0">
                                        <buttons>
                                            <Button mnemonicParsing="false" onMouseClicked="#threadTestButton" text="Thread" />
                                            <Button mnemonicParsing="false" text="Button" />
                                            <Button mnemonicParsing="false" text="Button" />
                                        </buttons>
                                    </ButtonBar>
                                </children>
                            </AnchorPane>
                        </content>
                    </Tab>
                    <Tab text="Untitled Tab 2">
                        <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                        </content>
                    </Tab>
                </tabs>
            </TabPane>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
